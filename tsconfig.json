{
  "compilerOptions": {
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "api/*": ["./src/api/*"],
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "state/*": ["./src/state/*"],
      "styles/*": ["./src/styles/*"],
      "utilities/*": ["./src/utilities/*"]
    }
  }
}
